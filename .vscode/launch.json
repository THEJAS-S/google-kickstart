{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Java",
      "type": "java",
      "request": "launch",
      "mainClass": "${file}",
      "console": "integratedTerminal",
      "cwd": "${fileDirname}",
      "args": [
        "<${fileDirname}/.stdin"
      ]
    },
    {
      "name": "Python",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "<${fileDirname}/.stdin"
      ]
    },
    {
      "name": "C++",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/Solution.out",
      "args": [
        "<${fileDirname}/.stdin"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build C++",
      "miDebuggerPath": "/usr/bin/gdb"
    }
  ]
}